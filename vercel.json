{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "ignore": [
    "corregedoria_painel.sql",
    "package-lock.json"
  ],
  "static": {
    "directory": "public"
  }
}
